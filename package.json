{
  "name": "artnet2atem",
  "version": "1.5.0",
  "description": "Electron artnet2atem",
  "engines": {
    "node": "<0.12"
  },
  "main": "main.js",
  "scripts": {
    "start": "gulp && electron .",
    "postinstall": "bower install && jspm install",
