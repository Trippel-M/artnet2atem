    "debug": "*",
    "electron": "^1.7.8",
    "electron-download": "^4.1.0",
    "fs": "*",
    "lodash": "^4.17.4",
    "jquery": "*"
  },
  "jspm": {
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core",
      "babel-runtime": "npm:babel-runtime",
      "core-js": "npm:core-js"
    }
  },
  "jest": {
    "collectCoverage": true,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
